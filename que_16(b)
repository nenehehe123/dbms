-- PL/pgSQL block to generate Fibonacci series
DO $$
DECLARE
    n INT := 10;  -- Change this for more terms
    a INT := 0;
    b INT := 1;
    c INT;
    i INT := 1;
BEGIN
    RAISE NOTICE 'Fibonacci Series (% terms):', n;
    RAISE NOTICE '%', a;
    RAISE NOTICE '%', b;

    FOR i IN 3..n LOOP
        c := a + b;
        RAISE NOTICE '%', c;
        a := b;
        b := c;
    END LOOP;
END;
$$;
